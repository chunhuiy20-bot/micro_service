# 现有配置
SERVICE_URLS_DICT={"user_service": "http://localhost:8001", "order_service": "http://localhost:8002"}
REQUEST_TIMEOUT=30.0

# ==================== Nacos 配置 ====================
# Nacos 服务器地址
NACOS_SERVER_ADDRESSES=127.0.0.1:8848

# Nacos 命名空间（可选，默认为空）
NACOS_NAMESPACE=

# 服务名称
NACOS_SERVICE_NAME=gateway-service

# 服务 IP（可选，默认自动获取本机 IP）
# NACOS_SERVICE_IP=192.168.1.100

# 服务端口
NACOS_SERVICE_PORT=8000

# 服务分组
NACOS_GROUP_NAME=DEFAULT_GROUP

# 服务权重
NACOS_WEIGHT=1.0

# 服务元数据（JSON 格式）
NACOS_METADATA={"version": "1.0.0", "env": "prod", "team": "backend"}

# Nacos 认证（可选，如果 Nacos 未开启认证则不需要配置）
# NACOS_USERNAME=nacos
# NACOS_PASSWORD=nacos

# 心跳间隔（秒）
NACOS_HEARTBEAT_INTERVAL=5

# 是否启用 Nacos 注册（true/false）
NACOS_ENABLED=true

# 是否为临时实例（true/false）
NACOS_EPHEMERAL=true